{
  "name": "donationApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "i": "yarn && ( cd ios && bundle exec pod install --repo-update) && yarn fix-images",
    "cleani": "rm -rf node_modules && rm -rf ./ios/Pods && yarn && ( cd ios && bundle exec pod install ) && yarn fix-images",
    "fix-images": "npx react-native-fix-image",
    "podsUpdate": "cd ios && rm -rf Pods && rm -rf Podfile.lock && pod install && cd .. && rm -rf build && react-native run-ios",
    "gradleUpdate": "cd android && ./gradlew clean && cd .. && react-native run-android"
  },
